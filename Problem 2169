class Solution {
    void solve(int num1, int num2, int& count) {

        if (num1 == 0 || num2 == 0) {
            return;
        }
        count++;
        if (num1 >= num2) {

            solve(num1 - num2, num2, count);
        } else {

            solve(num1, num2 - num1, count);
        }
    }

public:
    int countOperations(int num1, int num2) {
        int count = 0;
        solve(num1, num2, count);
        return count;
    }
};
